#!{

    @@name="supportiety-server"
    executable {

        services {
            "server.http"
            "server.manage"
            "server.persistence"
            "server.query"
        }

        generator go {
            package "example.com/golangee/supportiety"

            lib {
                id "json"
                name "encoding/json"
                version "1.17"
                type "core"
            }
            lib {
                id "xml"
                name "encoding/xml"
                version "1.17"
                type "core"
            }
            lib {
                id "http"
                name "net.http"
                version "1.*"
                type "io"
            }

            build {
                darwin "amd64"
                darwin "arm64"
                linux  "amd64"
            }
        }

    }
}